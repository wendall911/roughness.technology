{
    "useTabs": false,
    "singleQuote": true,
    "printWidth": 100,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true
}
